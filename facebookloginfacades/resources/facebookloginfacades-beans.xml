<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	
	<bean class="com.app.facebookloginfacades.data.SocialCustomerData">
		<property name="email" type="java.lang.String" />
		<property name="firstName" type="java.lang.String" />
		<property name="lastName" type="java.lang.String" />
		<property name="gender" type="java.lang.String" />
		<property name="mobileNumber" type="java.lang.String" />
		<property name="facebookUserId" type="java.lang.String" />
		<property name="socialNetworkProvider" type="java.lang.String" />
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.user.data.RegisterData">
		<property name="socialNetworkUserId" type="String" />
		<property name="socialNetworkProvider" type="String" />
		<property name="registrationSource" type="String" />
	</bean>

</beans>
